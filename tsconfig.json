{
  "compilerOptions": {
    "target": "esnext",
    "module": "Node16",
    "lib": [
      "esnext",
      "dom"
    ],
    "jsx": "react-native",
    "incremental": true,
    "esModuleInterop": true,
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": false,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@navigation/*": [
        "./src/navigation/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@types/*": [
        "./src/types/*"
      ]
    },
    "outDir": "./dist",
    "removeComments": true,
    "noEmit": true,
    "jsxFactory": "React.createElement",
    "jsxFragmentFactory": "Fragment"
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    "**/*.spec.ts",
    "**/*.test.ts",
    "**/__tests__/**",
    "**/__mocks__/**",
    "android",
    "ios",
    "web-build"
  ],
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "App.tsx",
    "index.js","src/**/*", "assets/**/*",
    "global.d.ts",
    "**/*.d.ts",
    "src",
    "declarations.d.ts"
  ],
  "extends": "expo/tsconfig.base"
}
