{
  "entryPoints": [
    "./app/screens/ArticlesScreen.tsx",
    "./app/screens/goalscreen.tsx",
    "./app/screens/homescreen.tsx",
    "./app/screens/index.tsx",
    "./app/screens/loginScreen.tsx",
    "./app/screens/notificationsscreen.tsx",
    "./app/screens/progressScreen.tsx",
    "./app/screens/scanscreen.tsx",
    "./context/AuthContext.tsx",
    "./app/_layout.tsx",
  ], // Points d'entrée pour la documentation
  "out": "./docs", // Dossier de sortie pour la documentation
  "exclude": [
    "**/node_modules/**",
    "**/tests/**",
    "**/assets/**",
    "**/.expo/**"
  ], // Exclure les dossiers inutiles
  "includeVersion": true, // Inclure la version du projet dans la documentation
  "name": "Green Habit", // Nom du projet
  "readme": "./README.md", // Inclure le fichier README.md
  "theme": "default", // Thème de la documentation
  "excludeExternals": true, // Exclure les modules externes
  "excludePrivate": true, // Exclure les membres privés
  "excludeProtected": true, // Exclure les membres protégés
  "hideGenerator": true // Masquer le message "Generated by TypeDoc"
}